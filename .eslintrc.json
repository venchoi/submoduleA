{
  "env": {
    "browser": true,
    "es6": true
  },
  "root": true,
  "extends": ["plugin:vue/essential", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "indent": 2,
    "prettier/prettier": "error",
    "no-var": "error",
    "semi": "error"
  }
}
